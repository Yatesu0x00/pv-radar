        <TextBox x:Name="tbIp4" Height="23" TextWrapping="Wrap" Text="56" Width="34" Canvas.Left="173" Canvas.Top="73" GotFocus="Tb_GotFocus"/>
        <Label Content="." Canvas.Left="113" Canvas.Top="78"/>
        <Label Content="." Canvas.Left="160" Canvas.Top="78"/>
        <Label Content="." Canvas.Left="60" Canvas.Top="78"/>
        <TextBox x:Name="tbPort" Height="23" TextWrapping="Wrap" Text="8888" Width="41" Canvas.Left="274" Canvas.Top="73" RenderTransformOrigin="0.819,0.174" Background="White" IsReadOnly="True"/>
        <Button x:Name="btnOk" Content="OK" Width="75" Canvas.Left="26" Canvas.Top="126" Click="btnOk_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" Width="75" Canvas.Left="160" Canvas.Top="126" Click="btnCancel_Click"/>
    </Canvas>
</Window>

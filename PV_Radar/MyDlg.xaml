        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PV_Radar"
        mc:Ignorable="d"
        Title="MyDlg" Height="198.398" Width="339.131">
    <Canvas Background="Gainsboro">
        <Label x:Name="lbIP" Content="IP-Adresse" Canvas.Left="26" Canvas.Top="42"/>
        <Label x:Name="lbPort" Content="Port" Canvas.Left="274" Canvas.Top="42"/>
        <TextBox x:Name="tbIp1" Height="23" TextWrapping="Wrap" Text="192" Width="34" Canvas.Left="26" Canvas.Top="73" GotFocus="Tb_GotFocus"/>
        <TextBox x:Name="tbIp2" Height="23" TextWrapping="Wrap" Text="168" Width="34" Canvas.Left="79" Canvas.Top="73" IsUndoEnabled="True" GotFocus="Tb_GotFocus"/>
